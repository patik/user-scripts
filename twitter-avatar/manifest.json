{
    "manifest_version": 2,
    "name": "Twitter Menu Avatar",
    "version": "2.2.0",
    "description": "Quickly verify which account you're logged into on Twitter.com",
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*", "*://*.twitter.com/*"],
            "js": ["twitter-avatar.user.js"],
            "run_at": "document_end"
        }
    ]
}
